{
  "name": "@haileyok/bluesky-video",
  "version": "0.1.7",
  "description": "A video player library for Bluesky",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android"
  },
  "keywords": [
    "react-native",
    "expo",
    "bluesky-video",
    "BlueskyVideo"
  ],
  "repository": "https://github.com/bluesky-social/bluesky-video",
  "bugs": {
    "url": "https://github.com/bluesky-social/bluesky-video/issues"
  },
  "author": "Bluesky Social <hailey@blueskyweb.xyz> (https://github.com/bluesky-social)",
  "license": "MIT",
  "homepage": "https://github.com/bluesky-social/bluesky-video#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.0.25",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "expo-module-scripts": "^3.5.2",
    "expo-modules-core": "^1.12.20",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react-native": "0.74.5"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "plugin:react/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "react/no-unescaped-entities": 0,
      "react-native/no-inline-styles": 0,
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "none",
          "useTabs": false,
          "semi": false,
          "arrowParens": "avoid",
          "bracketSameLine": true,
          "bracketSpacing": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "semi": false,
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": false
  }
}
